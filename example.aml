/begin A2ML
    taggedstruct protocol_layer {
        block "PROTOCOL_LAYER" struct {
            uint;
        };
    };
    enum custom_enum {
        "E1" = 0,
        "E2" = 2
    };
    block "IF_DATA" taggedunion {
        "XCP" struct {
            taggedstruct protocol_layer;
        };
        "CUSTOM" struct {
            enum custom_enum;
        };
    };
/end A2ML